//JavaScript for Arithmetic Operators
var a;
a='hi'+6;
console.log(typeof a);
console.log(a);
a='6'+6;
console.log(typeof a);
console.log(a);
a=6+6;
console.log(typeof a);
console.log(a);
a=true+true;
console.log(typeof a);
console.log(a);
a=6-3;
console.log(typeof a);
console.log(a);
a='hi'-'how';
console.log(typeof a);
console.log(a);
a='12'-9;
console.log(typeof a);
console.log(a);
a='hi'/'how';
console.log(typeof a);
console.log(a);
a='hi'*'how';
console.log(typeof a);
console.log(a);
a='12'/9;
console.log(typeof a);
console.log(a);
a='12'*9;
console.log(typeof a);
console.log(a);
a='12'+9;
console.log(typeof a);
console.log(a);

Output:
string
hi6
string
66
number
12
number
