//JavaScript for Bubble Sort
var a=[10,4,1,6,9,2,8,7,5,3,0];
var temp;
for(i=0;i<=a.length-1;i++)
{
  if(a[i]>a[i+1])
  {
    temp=a[i];
    a[i]=a[i+1];
    a[i+1]=temp;
  }
console.log(a);
}

Output:
[4,10,1,6,9,2,8,7,5,3,0]
[4,1,10,6,9,2,8,7,5,3,0]
[4,1,6,10,9,2,8,7,5,3,0]
[4,1,6,9,10,2,8,7,5,3,0]
[4,1,6,9,2,10,8,7,5,3,0]
[4,1,6,9,2,8,10,7,5,3,0]
[4,1,6,9,2,8,7,10,5,3,0]
[4,1,6,9,2,8,7,5,10,3,0]
[4,1,6,9,2,8,7,5,3,10,0]
[4,1,6,9,2,8,7,5,3,0,10]
[4,1,6,9,2,8,7,5,3,0,10]
