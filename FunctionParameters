//JavaScript for passing function parameters

function sai(a,b,c)
{
console.log(a);
console.log(b);
console.log(c);
}
sai(1,2,3);
sai(10,11,12);
function mul(a,b,c)
{
console.log('multiplication function');
console.log('mul before return');
return (a*b*c);
console.log('mul after return');
}

var result=mul(10,11,12);
console.log(result	);

Output:
1
2
3
10
11
12
multiplication function
mul before return
1320
