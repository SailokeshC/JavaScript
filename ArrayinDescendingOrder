//JavaScript for arranging elements in an array in Descending Order
var a=[10,4,1,6,9,2,8,7,5,3,0];
var temp;
for(j=0;j<a.length;j++)
{
 for(i=0;i<=a.length-1;i++)
 {
  if(a[i]<a[i+1])
  {
    temp=a[i];
    a[i]=a[i+1];
    a[i+1]=temp;
  }

 }
 console.log(a);
 console.log('------------');
}

Output:
[10,4,6,9,2,8,7,5,3,1,0]
