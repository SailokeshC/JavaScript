//JavaScript to find the string operations
var a="sai lokesh";
console.log(a.length);
console.log(a.toUpperCase());
console.log(a.toLowerCase());
console.log(a.repeat(5));
console.log(a.indexOf(' '));
console.log(a.indexOf('k'));
console.log(a.indexOf('s'));
console.log(a.lastIndexOf('s'));
var b="I am a masters student";
console.log(b.split(' '));
var c ="           I am a masters student";
console.log(c);
console.log(c.trim());
console.log(c.trimRight());
console.log(b.substring(3,13));
var d=a+b;
console.log(d);


Output:
10
